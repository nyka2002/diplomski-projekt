    "autoprefixer": "^10.4.24",
    "bullmq": "^5.67.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "ioredis": "^5.9.2",
    "lucide-react": "^0.563.0",
    "next": "15.2.1",
    "next-themes": "^0.4.6",
    "openai": "^6.17.0",
    "playwright": "^1.58.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
